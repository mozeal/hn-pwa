{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/my-view1.html",
    "src/my-view2.html",
    "src/my-view3.html",
    "src/my-view4.html",
    "src/my-view5.html",
    "src/my-view404.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": [
      "polymer-2"
    ]
  },
  "builds": [
    {
      "name": "bundled",
      "bundle": true,
      "js": {
        "minify": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "addServiceWorker": true
    },
    {
      "name": "unbundled",
      "js": {
        "minify": false
      },
      "css": {
        "minify": false
      },
      "html": {
        "minify": false
      },
      "addServiceWorker": true
    },
    {
      "preset": "es5-bundled",
      "basePath": true
    },
    {
      "preset": "es6-bundled",
      "basePath": false
    },
    {
      "preset": "es6-unbundled",
      "basePath": true
    }
  ]
}
